<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
        "-//Puppy Crawl//DTD Suppressions 1.1//EN"
        "http://checkstyle.sourceforge.net/dtds/suppressions_1_1.dtd">

<suppressions>
    <!-- Exception for library codes -->
    <suppress checks=".*" files="io/yggdrash/common/crypto/*"/>
    <suppress checks=".*" files="io.yggdrash.common.rlp/*"/>
    <suppress checks=".*" files="io/yggdrash/common/util/Utils.java"/>
